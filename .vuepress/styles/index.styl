@import url('https://rsms.me/inter/inter.css');

body
  background-color: #fafafc
  font-family: 'Inter', sans-serif
  color: #000000

.custom-block
  .custom-block-title
    font-weight 600
    margin-bottom -0.4rem
  &.note
    padding .1rem 1.5rem
    border-left-width .5rem
    border-left-style solid
    margin 1rem 0
  &.note
    background-color #f3f5f7
    border-color #1e90ff
.code-copy
    position: absolute;
    right: 0;
    bottom: 5px;
